<body>

    <div class="account-pages"></div>
    <div class="clearfix"></div>
    <div class="wrapper-page">
        <div class="card-box">
            <div class="panel-heading">
                <h4 class="text-center"> <strong class="text-custom">DOCTOR SIGNUP</strong> </h4></br></br>
                <center><img class="logo" style="height: 75px" src=@Url.Content(" /Content/myTemplate/assets/images/Our/web.png")></center>
            </div>

            <div class="panel-body">
                <form class="form-horizontal m-t-20" action="SignupDoctor" method="post" enctype="multipart/form-data" >
                    <div class="form-group ">
                        <div class="col-xs-12">
                            <input class="form-control" type="text" pattern="D-[0-9]{7}" title="Please enter valid registration number" required="" placeholder="Registration Number" name="RegNO">
                        </div>
                    </div>
                    <div class="form-group ">
                        <div class="col-xs-12">
                            <input class="form-control" type="text" required="" placeholder="Name" name="DocName">
                        </div>
                    </div>
                    <div class="form-group ">
                        <div class="col-xs-12">
                            <input class="form-control" type="email" required="" placeholder="Email" name="Email">
                        </div>
                    </div>
                  
                    <div class="form-group ">
                        <div class="col-xs-12">
                            <input class="form-control" type="password" pattern="(?=.*\d)(?=.*[a-z])(?=.*[A-Z]).{8,}" title="Must contain at least one number and one uppercase and lowercase letter, and at least 8 or more characters" required="" placeholder="Password" name="Password">
                        </div>
                    </div>
                    <div class="form-group ">
                        <div class="col-xs-12">
                            <textarea class="form-control" required="" placeholder="Your introduction here." name="Intro"></textarea>
                        </div>
                    </div>
                    <div class="form-group ">
                        <div class="col-xs-12">
                            <input type="file" class="filestyle"name="Photo" data-iconname="fa fa-cloud-upload" id="filestyle-6" tabindex="-1" style="position: absolute; clip: rect(0px 0px 0px 0px);">
                            <div class="bootstrap-filestyle input-group">
                                <input type="text" class="form-control " placeholder="Upload Your Photo" disabled="">
                                <span class="group-span-filestyle input-group-btn" tabindex="0">
                                    <label for="filestyle-6" class="btn btn-default ">
                                        <span class="icon-span-filestyle fa fa-cloud-upload"></span>
                                        <span class="buttonText">Choose file</span>
                                    </label>
                                </span>
                            </div>
                        </div>
                    </div>
                    <div class="form-group ">
                        <div class="col-xs-12">
                            <input class="form-control" type="text" required="" placeholder="Documents URL" name="Documents">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-2 control-label">Gender: </label>
                        <div class="col-md-10">
                            <div class="radio radio-info radio-inline">
                                <input type="radio" id="inlineRadio1" value="Male" name="Gender">
                                <label for="inlineRadio1"> Male </label>
                            </div>
                            <div class="radio radio-inline">
                                <input type="radio" id="inlineRadio2" value="Female" name="Gender">
                                <label for="inlineRadio2"> Female </label>
                            </div>
                        </div>
                    </div>
                    <div class="form-group ">
                        <div class="col-xs-12">
                            <input class="form-control" type="text" required="" pattern="[789][0-9]{9}" title="Please enter valid phone number" minlength="10" maxlength="10" placeholder="Mobile Number" name="Mobile">
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-xs-12">
                            Date of Birth
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-xs-12">
                            <input class="form-control" type="date" name="DOB">
                        </div>
                    </div>
                   
                    

                    <div class="form-group">
                        <div class="col-xs-12">
                            Speciality
                        </div>
                    </div>
                    <div class="form-group ">
                        <div class="col-xs-12">
                            <select class="form-control" name="SpecialityId">
                                @for (int i = 0; i < sp.Rows.Count; i++)
                                {
                                <option value=@sp.Rows[i]["SpecialityId"]>@sp.Rows[i]["SpName"]</option>
                                
                                }
                            </select>
                        </div>
                    </div>
                    <div class="form-group ">
                        <div class="col-xs-12">
                            <input class="form-control" type="text" required="" placeholder="Qualification" name="Qualification">
                        </div>
                    </div>
                    <div class="form-group ">
                        <div class="col-xs-12">
                            <input class="form-control" type="number" required="" placeholder="Minimum Consultation Fee" name="mcf">
                        </div>
                    </div>
                    <div class="form-group ">
                        <div class="col-xs-12">
                            <input class="form-control" type="number" required="" placeholder="Maximum appoinments per hour" name="maph">
                        </div>
                    </div>
                    <div class="form-group text-center m-t-40">
                        <div class="col-xs-12">
                            <button class="btn btn-pink btn-block text-uppercase waves-effect waves-light" type="submit">
                                Register
                            </button>
                        </div>
                    </div>
                </form>

            </div>
        </div>

        <div class="row">
            <div class="col-sm-12 text-center">
                <p>
                    Already have account?<a href="/Access/Loginuser" class="text-primary m-l-5"><b>Sign In</b></a>
                </p>
            </div>
        </div>

    </div>

