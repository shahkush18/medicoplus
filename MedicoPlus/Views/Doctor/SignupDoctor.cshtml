    <script src=@Url.Content("~/Content/myTemplate/assets/js/jquery.slimscroll.js")></script>
    <script src=@Url.Content("~/Content/myTemplate/assets/js/jquery.blockUI.js")></script>
    <script src=@Url.Content("~/Content/myTemplate/assets/js/waves.js")></script>
    <script src=@Url.Content("~/Content/myTemplate/assets/js/wow.min.js")></script>
    <script src=@Url.Content("~/Content/myTemplate/assets/js/jquery.nicescroll.js")></script>
    <script src=@Url.Content("~/Content/myTemplate/assets/js/jquery.scrollTo.min.js")></script>


    <script src=@Url.Content("~/Content/myTemplate/assets/js/jquery.core.js")></script>
    <script src=@Url.Content("~/Content/myTemplate/assets/js/jquery.app.js")></script>

